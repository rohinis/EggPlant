
params setting

Log  "Access Settings"
repeat while imageFound("PD_Icn_Settings")
	--	log "In repeat"
	moveto foundImageLocation()
	--	wait 3
	Click  foundImageLocation()
	exit repeat 
end repeat


if setting=1 then
	Log "Show Job Profile"
	Click (image:"PD_HomePage_Lables/Lable_ShowJobProfiles")
	
else if setting =2 then
	
	--Set the SearchRectangle to (1620,0,1920,123)
	Log  "OpenCluster"
	Click (Text:"Clusters")
	--Set the SearchRectangle to ()
	
else if setting = 3
	Log "Export Config"
	Click (Text: "Export Config")
	
else if setting= 4
	log "Show logs"
	Click (Text:"Show Logs")
else if setting = 5 
	log "Remote Drive"
	Click (image: "PD_HomePage_Lables/Lable_RemoteDrive",scale: 0.5 to 1.5 by .25)
	
else if setting =6 
	Log "Help"
	Click (Text: "Help") 
	
else if setting =7
	Log "Show Applications"
	Click (Text: "Show Applications")
	
else if setting =8
	
	Log "Prefrences"
	Click (Text: "Preferences")
else
	Log "Exit"
	Click (image:"PD_HomePage_Lables/Lable_Exit",scale: 0.5 to 1.5 by .25)
end if 
